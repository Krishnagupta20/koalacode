# quick sample
nums = [1, 2, 3, 4];
give(nums[2]);

x = 5;
this (x > 3) {
    give("greater");
} otherwise {
    give("smaller");
}

i = 0;
iter(i < 3) {
    give(i);
    i = i + 1;
}
func fib(n) {
    this(n <= 1) {
        return n;
    } otherwise {
        return fib(n - 1) + fib(n - 2);
    }
}

give(fib(10));
